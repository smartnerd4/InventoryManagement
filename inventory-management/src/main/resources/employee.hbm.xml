<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.smartnerd.model.employee" table="EMPLOYEE_DETAILS">
		<id name="EMP_ID" type="string">
			<column name="EMP_ID" length="10" not-null="true" />
			<generator class="assigned" />
		</id>
		<property name="EMP_NAME" type="string">
			<column name="EMP_NAME" length="30" not-null="false" />
		</property>
		<property name="EMAIL_ID" type="string">
			<column name="EMAIL_ID" length="30" not-null="false" />
		</property>
		<property name="DEPT_ID" type="string">
			<column name="DEPT_ID" length="10" not-null="false" />
		</property>
		<property name="DOJ" type="date">
			<column name="DOJ" length="7" not-null="false" />
		</property>
		<property name="PAN_NUMBER" type="string">
			<column name="PAN_NUMBER" length="20" not-null="false" />
		</property>
		<property name="AADHAR_NUMBER" type="string">
			<column name="AADHAR_NUMBER" length="20" not-null="false" />
		</property>
		<property name="WORK_EXP" type="int">
			<column name="WORK_EXP"  not-null="false" />
		</property>
		<property name="PREV_ORG" type="string">
			<column name="PREV_ORG" length="20" not-null="false" />
		</property>
		<property name="RELEV_DATE" type="date">
			<column name="RELEV_DATE" length="7" not-null="false" />
		</property>
		<property name="REPORTING_ID" type="string">
			<column name="REPORTING_ID" length="10" not-null="false" />
		</property>
		<property name="MANAGER_ID" type="string">
			<column name="MANAGER_ID" length="10" not-null="false" />
		</property>
		<property name="HIGH_QUAL" type="string">
			<column name="HIGH_QUAL" length="20" not-null="false" />
		</property>
		<property name="SYS_CRE_DATE" type="date">
			<column name="SYS_CRE_DATE" length="7" not-null="false" />
		</property>
		<property name="SYS_LST_MOD_DATE" type="date">
			<column name="SYS_LST_MOD_DATE" length="7" not-null="false" />
		</property>
	</class>
</hibernate-mapping>